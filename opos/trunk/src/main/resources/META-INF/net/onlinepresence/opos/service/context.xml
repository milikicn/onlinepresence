<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="net.onlinepresence.opos.domain.service.Users" 
class="net.onlinepresence.opos.domain.service.beans.UsersBean" scope="prototype">
	<property name="reader">
		<ref bean="net.onlinepresence.opos.service.crud.impl.ReadCommand"/>
	</property>
	<property name="updater">
		<ref bean="net.onlinepresence.opos.service.crud.impl.UpdateCommand"/>
	</property>
	<property name="writer">
		<ref bean="net.onlinepresence.opos.service.crud.impl.WriteCommand"/>
	</property>
	<property name="delete">
		<ref bean="net.onlinepresence.opos.service.crud.impl.DeleteCommand"/>
	</property>
		<property name="membershipDeleter">
		<ref bean="net.onlinepresence.opos.service.crud.impl.DeleteCommand"/>
	</property>
	<property name="searcher">
		<ref bean="net.onlinepresence.opos.service.crud.impl.ExampleSearchCommand"/>
	</property>
</bean>
<bean id="net.onlinepresence.opos.domain.service.Applications" 
class="net.onlinepresence.opos.domain.service.beans.ApplicationsBean" scope="prototype">
	<property name="reader">
		<ref bean="net.onlinepresence.opos.service.crud.impl.ReadCommand"/>
	</property>
	<property name="updater">
		<ref bean="net.onlinepresence.opos.service.crud.impl.UpdateCommand"/>
	</property>
	<property name="writer">
		<ref bean="net.onlinepresence.opos.service.crud.impl.WriteCommand"/>
	</property>
	<property name="delete">
		<ref bean="net.onlinepresence.opos.service.crud.impl.DeleteCommand"/>
	</property>
	<property name="searcher">
		<ref bean="net.onlinepresence.opos.service.crud.impl.ExampleSearchCommand"/>
	</property>
</bean>
<bean id="net.onlinepresence.opos.domain.service.Keys" 
class="net.onlinepresence.opos.domain.service.beans.KeysBean" scope="prototype">
	<property name="reader">
		<ref bean="net.onlinepresence.opos.service.crud.impl.ReadCommand"/>
	</property>
	<property name="writer">
		<ref bean="net.onlinepresence.opos.service.crud.impl.WriteCommand"/>
	</property>
	<property name="delete">
		<ref bean="net.onlinepresence.opos.service.crud.impl.DeleteCommand"/>
	</property>
		<property name="updater">
		<ref bean="net.onlinepresence.opos.service.crud.impl.UpdateCommand"/>
	</property>
</bean>
<bean id="net.onlinepresence.opos.domain.service.Administrators" 
class="net.onlinepresence.opos.domain.service.beans.AdministratorsBean" scope="prototype">
	<property name="reader">
		<ref bean="net.onlinepresence.opos.service.crud.impl.ReadCommand"/>
	</property>
</bean>
</beans>