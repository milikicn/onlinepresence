<html t:type="layout" title="Admin"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
      xmlns:p="tapestry:parameter">

    <div class="form">
        <form t:type="Form" t:id="loginForm"><t:errors />
        	<div class="formRow">
                <label class="label">Username</label>
                <div class="inputWrapper">
                    <div class="inputLeft"></div>
                    <div class="inputCenter">
                        <input class="input" id="username" name="username" title="username" tabindex="4" type="text"
					                t:id="username" t:validate="required" t:type="TextField" t:value="username"/>
                    </div>
                    <div class="inputRight"></div>
                </div>
            </div>
            
            <div class="formRow">
                <label class="label">Password</label>
                <div class="inputWrapper">
                    <div class="inputLeft"></div>
                    <div class="inputCenter">
                         <input class="input" id="password" name="password" t:id="password" t:type="PasswordField" 
								t:value="password" t:validate="required" title="password" tabindex="5" />
                    </div>
                    <div class="inputRight"></div>
                </div>
            </div>
            
        	<input id="submit" class="btnSignIn" type="submit" value="Sign in" />
        </form>
    </div>

<div class="hr"></div>
</html>
