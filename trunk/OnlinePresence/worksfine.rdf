<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:foaf="http://xmlns.com/foaf/0.1/"
    xmlns:opo="http://ggg.milanstankovic.org/opo/ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
  <opo:OnlinePresence rdf:about="http://nekiURIZaOnlinePresence.com">
    <opo:customMessage>watching a game, having a bud</opo:customMessage>
    <opo:avatar rdf:resource="http://nikola.em3.rs/images/photo.jpg"/>
    <opo:hasPresenceComponent rdf:resource="#NotificationsConstrained"/>
    <opo:hasPresenceComponent rdf:resource="#PubliclyFindable"/>
    <opo:declaredBy>
      <foaf:Agent rdf:about="http://nekiURIZaAgenta.com">
        <foaf:img rdf:resource="http://mojaslika.com/slika.jpg"/>
        <foaf:name>nikola milikic</foaf:name>
      </foaf:Agent>
    </opo:declaredBy>
    <opo:hasPresenceComponent>
      <opo:OnlineStatus>
        <opo:hasStatusComponent rdf:resource="#Invisible"/>
        <opo:hasStatusComponent rdf:resource="#Active"/>
        <opo:hasStatusComponent rdf:resource="#FreelyContactable"/>
        <opo:hasStatusComponent rdf:resource="#Available"/>
      </opo:OnlineStatus>
    </opo:hasPresenceComponent>
  </opo:OnlinePresence>
</rdf:RDF>
